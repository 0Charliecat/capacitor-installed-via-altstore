{
  "name": "capacitor-read-native-setting",
  "version": "3.1.0",
  "description": "A simple plugin to read from Info.plist on iOS and strings.xml on Android",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Jay Hughes",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.9.0",
    "@capacitor/cli": "^3.9.0",
    "@capacitor/ios": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@capacitor/core": "3.9.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorReadNativeSetting.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ingageco/capacitor-read-native-setting"
  },
  "bugs": {
    "url": "https://github.com/ingageco/capacitor-read-native-setting/issues"
  }
}
